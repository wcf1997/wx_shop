<!--components/tabs/Tabs.wxml-->
<view class="tabs">
    <view class="tabs_title">
        <view class="title_item {{item.isActive?'active': ''}}"
         wx:for="{{tabs}}" wx:key="id" data-index="{{index}}" bindtap="itemClick">
        {{item.value}}
        </view>
    </view>
    <view class="tabs_content">
    </view>
</view>
